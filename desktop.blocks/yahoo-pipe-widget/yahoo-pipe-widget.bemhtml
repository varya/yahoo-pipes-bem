block yahoo-pipe-widget {

    tag: 'ul'

    js: {
        pipeId: this.ctx.pipeId
    }

    elem item {

        tag: 'li'

    }

    elem title {

        tag: 'h3'

    }

    elem pic {

        tag: '',
        tag, this.ctx.url: 'img'
        attrs: {
            src: this.ctx.url,
            border: 0
        }
    }

    elem link {
        tag: 'a',
        attrs: {
            href: this.ctx.url
        },
        content: 'full version'
    }

}
