block yahoo-pipe-widget {

    tag: 'ul'

    js: {
        pipeId: this.ctx.pipeId
    }

    elem item {

        tag: 'li'

    }

    elem title {

        tag: 'h3'

    }

    elem pic {
        tag: 'img',
        attrs: {
            src: this.ctx.url,
            border: 0
        }
    }

}
